    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "resolve": "1.6.0",
    "s": "^0.1.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "ts-jest": "22.0.1",
    "ts-polyfill": "^3.0.1",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "undux": "^5.0.0-alpha.15",
    "url-loader": "0.6.2",
    "utility-types": "^2.1.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@types/quill": "^1.3.10",
    "@types/react-intl": "^2.3.11",
    "@types/react-test-renderer": "^16.0.2",
    "@types/react-virtualized-select": "^3.0.7",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/enzyme": "^3.1.13",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-loadable": "^5.4.1",
    "@types/react-redux": "^6.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-mock-store": "^1.0.0",
    "@types/webpack": "^4.4.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.4",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "express-history-api-fallback": "^2.2.1",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react-dev-utils": "6.0.0-next.3e165448",
    "react-hot-loader": "^4.3.6",
    "redux-mock-store": "^1.5.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^4.5.0",
    "ts-simple-ast": "^14.4.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typesafe-actions": "^2.0.4",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.8",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "rekit": {
    "devPort": 6075,
    "studioPort": 6076,
    "plugins": [],
    "css": "less"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/tests/setup.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/tests/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "@ajax": "<rootDir>/src/utils/new-ajax",
      "@src(.*)$": "<rootDir>/src/$1",
      "@features(.*)$": "<rootDir>/src/features/$1",
      "^react-native$": "react-native-web",
      "quill": "<rootDir>/src/features/common/quill.mock.test.ts"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "ie": "9"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "syntax-dynamic-import",
      "react-loadable/babel",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
